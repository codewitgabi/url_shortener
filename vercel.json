{
  "version": 2,
  "builds": [
    {
      "src": "urlshortener/wsgi.py",
      "use": "@ardnt/vercel-python-wsgi",
      "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "urlshortener/wsgi.py"
    }
  ]
}
